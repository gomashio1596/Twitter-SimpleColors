{
    "name": "Twitter-SimpleColors",
    "version": "1.2.1",
    "manifest_version": 2,
    "description": "Change Twitter colors",
    "icons": {
        "128": "128.png"
    },
    "content_scripts": [{
        "matches": [
            "https://twitter.com/*"
        ],
        "js": [
            "content.js"
        ],
        "run_at": "document_start"
    }]
}